    "@mui/material": "^5.10.14",
    "@mui/system": "^5.10.14",
    "@mui/x-date-pickers": "^5.0.8",
    "@next/mdx": "^13.0.6",
    "@tanstack/react-query": "^4.19.1",
    "@tanstack/react-query-devtools": "^4.19.1",
