{
  "name": "acnw",
  "version": "0.1.0",
  "description": "AmberCon NW site",
  "author": "guy@wyrdrune.com",
  "license": "MIT",
  "type": "module",
  "private": true,
  "files": [],
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "boot": "DB_ENV=acnw pnpm --filter database db:migrate:up && DB_ENV=acnw pnpm --filter amber boot ",
    "build": "pnpm boot && pnpm next:build",
    "next:build": "NODE_ENV=production next build",
    "db:import": "DB_ENV=acnw pnpm --filter database db:import",
    "dev": "pnpm boot && NODE_OPTIONS=--max_old_space_size=4096 next dev -p 30000",
    "start": "next start -p 30000",
    "tsc": "tsc -p ./tsconfig.json",
    "tsgo": "tsgo -p ./tsconfig.json"
  },
  "dependencies": {
    "@amber/amber": "workspace:*",
    "@amber/api": "workspace:*",
    "@amber/client": "workspace:*",
    "@amber/database": "workspace:*",
    "@amber/environment": "workspace:*",
    "@amber/server": "workspace:*",
    "@amber/ui": "workspace:*",
    "@auth0/nextjs-auth0": "4.9.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "@mui/system": "^7.3.2",
    "@mui/x-date-pickers": "^8.11.1",
    "@next/mdx": "^15.5.2",
    "@prisma/nextjs-monorepo-workaround-plugin": "^6.15.0",
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^3.5.0",
    "@tanstack/react-query": "^5.87.1",
    "@trpc/server": "^11.5.1",
    "debug": "4.4.1",
    "email-templates": "^12.0.3",
    "formik": "^2.4.6",
    "jose": "^6.1.0",
    "luxon": "^3.7.2",
    "micro-cors": "^0.1.1",
    "next": "15.5.2",
    "next-mdx-frontmatter": "^0.0.4",
    "pug": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "stripe": "^15.12.0",
    "yup": "^1.7.0",
    "zod": "^3.25.76",
    "zod-formik-adapter": "^1.3.0"
