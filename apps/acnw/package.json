    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@swc/core": "^1.13.5",
    "@types/debug": "4.1.12",
    "@types/luxon": "^3.7.1",
    "@types/mdx": "^2.0.13",
    "@types/micro-cors": "^0.1.5",
    "@types/node": "^24.3.1",
    "@types/pug": "^2.0.10",
