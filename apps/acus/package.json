{
  "name": "acus",
  "version": "0.1.0",
  "description": "AmberCon US site",
  "author": "guy@wyrdrune.com",
  "license": "MIT",
  "private": true,
  "files": [],
  "scripts": {
    "analyze": "ANALYZE=true pnpm build",
    "boot": "DB_ENV=acus pnpm --filter database db:migrate:up && DB_ENV=acus pnpm --filter amber boot && pnpm --filter client boot",
    "build": "pnpm boot && pnpm next:build",
    "next:build": "NODE_ENV=production next build",
    "db:import": "DB_ENV=acus pnpm --filter database db:import",
    "dev": "pnpm boot && next dev -p 30001",
    "lint": "eslint --cache --color '**/*.{js,jsx,ts,tsx}' ./.eslintrc.js",
    "start": "next start",
    "tsc": "tsc -p ./tsconfig.json",
    "test": "next test"
  },
